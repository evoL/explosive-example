{
  "name": "boom",
  "version": "0.0.1",
  "licence": "MIT",
  "main": "src/main.jsx",
  "browserOutput": "build/main.js",
  "layout": "index.html",
  "scripts": {
    "build": "browserify src/main.jsx -o build/main.js --extension .jsx",
    "watch": "watchify src/main.jsx -o build/main.js -d -v --extension .jsx"
  },
  "dependencies": {
    "babel-runtime": "^5.2.6",
    "explosive": "file:../explosive",
    "express": "^4.12.3",
    "react": "^0.13.2",
    "react-router": "^0.13.3",
    "reqwest": "^1.1.5"
  },
  "devDependencies": {
    "browserify": "^9.0.0",
    "watchify": "^3.1.0",
    "babelify": "^6.0.0"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "optional": [
            "runtime"
          ]
        }
      ]
    ]
  }
}
